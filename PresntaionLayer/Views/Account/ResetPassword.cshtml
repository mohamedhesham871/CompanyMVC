@model ResetPassword
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<h1>ResetPassword</h1>

<div class="main-w3layouts wrapper">
	<h1>Mohamed Hesham Elkady Site</h1>
	<div class="main-agileinfo">
		<div class="agileits-top">
			<form asp-action="ResetPassword" method="post">
				<div asp-validation-summary="All"></div>


				<input class="text" type="password" asp-for="Password" placeholder="Password" required="">
				<br />
				<input class="text" type="password" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
		
				<input type="submit" value="Reset">
			</form>
		</div>
	</div>
</div>
